@model TravelPackage;

<div class="card" style="width: 18rem;">
    <img src="@Model.CoverImage" class="card-img-top" alt="Image of @Model.Name">
    <div class="card-body">
        <h2>@Model.Name</h2>
        <span class="card-text d-block">@Model.Description</span>
        <span class="card-text d-block">@Model.Price</span>
    </div>
</div>